    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "mocha-loader": "^0.7.1",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "phantomjs": "^1.9.18",
    "postcss-loader": "^0.8.0",
    "react-addons-test-utils": "^0.14.7",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.1",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
