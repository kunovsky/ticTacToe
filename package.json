{
  "name": "ticTac",
  "version": "0.0.1",
  "description": "tic tac toe",
  "repository": "",
  "config": {
    "buildDir": "./build",
    "buildDirTests": "./build_tests",
    "devHost": "localhost",
    "devPort": 8080
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "lint": "eslint --ext .js --ext .jsx ./app && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "npm run clean",
    "pretest": "npm install && npm run lint",
    "start": "NODE_ENV=development parallelshell 'node ./webpack/servers/dev-server' 'webpack --colors --progress --config ./webpack/config'",
    "test": "NODE_ENV=test karma start",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "dependencies": {
    "flux": "^2.1.1",
    "foundation-sites": "^6.2.0",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-rewire": "^1.0.0-rc-1",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "glob": "^5.0.6",
    "history": "^1.13.1",
    "html-loader": "^0.3.0",
    "json-loader": "^0.5.1",
    "isparta": "^4.0.0",
    "isparta-loader": "2.0.0",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "0.1.1",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
